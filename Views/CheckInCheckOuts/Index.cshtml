@{
  string[] tableHeaders = {"ID USER", "Entry hour" , "Departure hour"};
}

<h2 class="mb-3 text-center">Login history</h2>

<table class="table table-striped">
  <thead>
    <tr>
      @foreach (var header in tableHeaders)
      {
        <th scope="col">@header</th>
      }
    </tr>
  </thead>

  <tbody>
    @foreach (var CheckIn in Model)
    {
      <tr>
        <td>@CheckIn.IdUser</td>
        <td>@CheckIn.EntryHour</td>
        <td>@CheckIn.DepartureHour</td>
        @* <td class="d-flex">
          <a asp-action="Detail" asp-route-id="@CheckIn.Id" class="btn btn-secondary">Details</a>
          <a asp-action="Update" asp-route-id="@CheckIn.Id" class="btn btn-primary">Update</a>
          <a asp-action="Delete" asp-route-id="@CheckIn.Id" class="btn btn-danger">Delete</a>
        </td> *@
      </tr>
    }
  </tbody>
</table>